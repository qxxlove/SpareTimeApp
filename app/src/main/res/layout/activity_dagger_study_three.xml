        />

    <TextView
        android:id="@+id/text_teacher_content_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_margin="@dimen/activity_dimens_ten"
        />

    <TextView
        android:id="@+id/text_dagger_study_four"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
