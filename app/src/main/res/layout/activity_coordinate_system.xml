    <TextView
        android:id="@+id/text_content_coordinate_activity_four"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="view自身的顶边到其父布局顶边的距离 "
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/activity_dimens_ten"
        />
    <TextView
        android:id="@+id/ll_content_coordinate_activity_one"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="view自身的顶边到其父布局顶边的距离 "
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/activity_dimens_ten"
        />
    <TextView
        android:id="@+id/ll_content_coordinate_activity_two"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="view自身的顶边到其父布局顶边的距离 "
